{{#each this}}
<li class="photo-card">
    <img src="{{webformatURL}}" data-src="{{largeImageURL}}" alt="" />

    <div class="stats">
        <p class="stats-item">
            <i class="material-icons icon">thumb_up</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons icon">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons icon">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons icon">cloud_download</i>
            {{downloads}}
        </p>
    </div>
</li>
{{/each}}